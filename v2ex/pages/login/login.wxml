<view class='login'>  
  <view>
    <swiper class='swiper-module' indicator-dots="{{false}}"  autoplay="{{false}}" interval="{{5000}}" duration="{{1000}}" circular="{{false}}">
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image"/>
      </swiper-item>
    </block>
  </swiper>
  </view>

  <view class='swiper-module-group'>
    <text class='swiper-module-group-text'>"步骤1"</text>
  </view>

  <view class='button-group'>  
    <view>
      <image src='/images/tabBar/f10.png'  class='button-group-img'></image>
      
    </view>
    <view>
      <button class='swiper-buttom' bindtap='jump'>下一页</button>
    </view>
    <view>
      <image src='/images/tabBar/f10.png' class='button-group-img'></image>
    </view>
  </view>
</view>
