.text-box-text-title{
  font-family: 'Times New Roman', Times, serif;
  font-size: 20pt;
  margin-bottom: 10px;
  background: #FFFAF0
}

.text-box-buttom{
  display: flex;
  font-family: Arial, Helvetica, sans-serif;
  justify-content: space-between;  
  background: #FFFAF0
}


.user{
  padding: 10px;
  background: #AEEEEE;
  height: 100%;
}

.user-Interface{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 10px;
}

.user-Interface-images{
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  margin-bottom: 20px;
}

.user-Interface-text{
  color: #888888;
  font-size: 15pt;
}

.user-textbox{
  background: #ffffff;
  padding: 10px;
  border-bottom: 5px solid #888888;
  border-radius: 2%;
  margin-bottom: 20rpx;
}

.user-textbox-top{

}

.user-textbox-top-remask{
  font-size: 12px;
  color: #353535;
}

.user-textbox-buttom{
  display: flex;
  justify-content: space-between;
}

.user-textbox-buttom-revert{
  color: #888888;
  font-size: 12px;
}

.user-textbox-buttom-review{
  color: #888888;
  font-size: 12px;
}